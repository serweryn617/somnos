struct [dral style="lowercase"]register.name[#dral]
{
  constexpr static uint8_t Address = [dral format="{:#04X}"]register.offset[#dral];
  constexpr static uint8_t RegBank = [dral format="{:#04X}"]register.reg_bank[#dral];
  union
  {
    uint8_t value;
    [dral template="bitfield.dral"]register.fields[#dral]
  };
};

